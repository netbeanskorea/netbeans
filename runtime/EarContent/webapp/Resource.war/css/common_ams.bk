@charset "utf-8";

@font-face {
	font-family:NanumGothic;
	src:url('../font/NanumGothic.eot');
	src: local('?'), 
	url('../font/NanumGothic.woff') format('woff');
}

@font-face {
    font-family:NanumGothicExtraBold; 
	src:url('../font/NanumGothicExtraBold.eot');
    src: local('?'), 
	url('../font/NanumGothicExtraBold.woff') format('woff');
}

/* common */
html{width:100%; height:100%; overflow-y:scroll;}
*{padding:0; margin:0; z-index:1;}
* html body{_behavior:url("csshover3.htc");}

body{width:100%; height:100%; color:#8e8e8e; font-size:12px; font-family:NanumGothic, Gulim,Dotum,Tahoma; font-weight:normal; line-height:17px;}
div, p, ul, li, table, th, td, textarea, input, span, form, h1, h2, h3, h4, h5, h6, dt, dl, dd{padding:0; margin:0; word-break:break-all; word-wrap:break-word; word-break:keep-all;}
ul, ol, dl, dd, dt, li{list-style-type:none; padding:0; margin:0;}
img{border:0;}
fieldset{border:0; margin:0px; padding:0px;}
legend{display:none; position:absolute; visibility:hidden;} 
caption, .blind {overflow:hidden;visibility:hidden;width:0px;height:0px;font-size:0px;line-height:0px;padding:0px;margin:0px;}
optgroup {height:0px; font-style:normal; font-size:0px; line-height:0px;}
table {border-collapse:separate; *border-collapse:collapse; border-spacing:0px; margin:0; padding:0; border:0;}
input{vertical-align:middle;}
textarea[readonly=readonly],
textarea[readonly],
input[readonly=readonly],
input[readonly]{background-color:#f3f3f3; color:#a9a9a9;}
input.nobd{border:0;}
.input01{border:1px solid #bebebe; width:97%;}

select[disabled=disabled],
select[disabled],
textarea[disabled=disabled],
textarea[disabled],
input[disabled=disabled],
input[disabled]{background-color:#f3f3f3; color:#a9a9a9;}

.readonly {background:transparent !important; border-right:0px none;}

/* scroll style */
html{
	scrollbar-3dLight-Color:#fff; 
	scrollbar-arrow-color:#B9B8B8; 
	scrollbar-base-color:#B9B8B8; 
	scrollbar-Face-Color:#fff; 
	scrollbar-Track-Color:#fff; 
	scrollbar-DarkShadow-Color:#ddd;
	scrollbar-Highlight-Color: #B9B8B8; 
	scrollbar-Shadow-Color: #B9B8B8;
}
/* Chrome, Safari용 스크롤 바 */
::-webkit-scrollbar{width: 8px; height: 8px; border: 3px solid #dfdfdf; }
::-webkit-scrollbar-button:start:decrement, ::-webkit-scrollbar-button:end:increment{display:block; height: 10px; background:#efefef none;}
::-webkit-scrollbar-track{background: #efefef; -webkit-border-radius: 10px; border-radius:10px; -webkit-box-shadow: inset 0 0 4px rgba(0,0,0,.2)}
::-webkit-scrollbar-thumb{height: 50px; width: 50px; background:#c6c6c6; -webkit-border-radius: 8px; border-radius: 8px; -webkit-box-shadow: inset 0 0 4px #c6c6c6}

/* 커서 */
.cursor{cursor:pointer;}

/* 링크 */
a{color:#8e8e8e;text-decoration:none;font-family:NanumGothic, Gulim,Dotum,Tahoma;}
a:link, a:visited{color:#8e8e8e;text-decoration:none;font-family:NanumGothic, Gulim,Dotum,Tahoma;}
a:hover, a:focus, a:active{color:#8e8e8e;text-decoration:underline;font-family:NanumGothic, Gulim,Dotum,Tahoma;}

/* ie8 버튼 버그 */
button:active{-ms-background-position-x:1px; -ms-background-position-y:1px;}

/* text 정렬 */
.txt_l{text-align:left !important;}
.txt_c{text-align:center !important;}
.txt_r{text-align:right !important;}

/* text type */
.fttype01{color:blue; font-weight:bold;} /* SB 파란색 */
.fttype01.addtype01{text-decoration:underline; font-weight:normal;}
.fttype02{color:#c08236; font-weight:bold;} /* SB 빨간색 */
.underline{text-decoration:underline;}

/* float */
.float_l{float:left;}
.float_r{float:right;}

/* clear */
.clearfix{display:block;*display:inline-block;}
.clearfix:after {content:".";display:block;height:0;clear:both;visibility:hidden;*zoom:1;}
/* Hides from IE-mac \*/
* html .clearfix {height: 1%;}
/* End hide from IE-mac */

/*input*/
.radio, .checkbox {width:12px;height:12px;margin:0 3px 0 0;vertical-align:middle;}
.radio{margin-top:-2px;}
label{margin-right:5px;}
textarea {border:1px solid #c4baaf;padding:5px 0 5px 10px;font-size:12px;color:#666;line-height:17px;}
select {color:#383838; font-family:NanumGothic, Gulim,Dotum,Tahoma; font-size:12px; border:1px solid #c2c2c2; padding: 1px 1px 1px 2px;}
