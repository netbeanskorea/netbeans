/* text image */
em{visibility:hidden;font-size:0;line-height:0;position:absolute;display:inline-block;}

/* 스크롤 스타일 */
.scroll {overflow-x:hidden;overflow-y:scroll;scrollbar-face-color:#fff;scrollbar-track-color:#fafafa;scrollbar-arrow-color:#adadad;scrollbar-highlight-color:#fff;scrollbar-3dlight-color:#ddd;scrollbar-shadow-color:#ddd;scrollbar-darkshadow-color:#fff;}

/* layout */
#container{position:relative;}
#headerArea{position:relative;}
#globalArea{}
.global{position:relative; width:1200px; height:54px; margin:0 auto;}
#gnbArea{background:url(../images/top/bg_gnb.png) repeat-x left bottom;}
.gnb{position:relative; width:1200px; margin:0 auto;}
#wrapper{position:relative; width:1200px; margin:0 auto;}
/*#wrapper::after{content:" "; display:block; clear:both; height:0; visibility:hidden;}*/
#leftArea{float:left; display:inline; width:193px; padding-top:30px;}
#contentArea, #contents{float:left; display:inline; background:url(../images/common/bg_content.png) no-repeat 400px top; margin-left:22px; width:831px; padding-top:30px;}

/* div type */
.div_type01{border:2px solid #c6c6c6; padding:5px;}


/* Button - 기본버튼 */
.buttonLayer{text-align:right;}
.btn_main{margin-top:30px; text-align:center;}
.btn_search{margin-top:10px; text-align:right;}
.btn_common{margin-top:20px; text-align:right;}
.btn_common.type02{float:right; margin-top:-20px;} /* 타이틀과 같은 라인에 위치 */
.btn_common.type02 .input01.num03{width:76px;}
.btn_grid{margin-top:20px; text-align:left;}

.button,.button a,.button button,
.button input{position:relative; margin:0; display:-moz-inline-stack; display:inline-block; text-decoration:none; border:0; font-family:NanumGothic, Gulim,Dotum,Tahoma; font-size:12px; white-space:nowrap; background:url(../images/button/btn_basic02.png) no-repeat; overflow:visible; color:#6e6e6e; height:29px;}
.button{padding:0; vertical-align:middle; margin-right:3px; background-position:left top;}
.button a,.button button,.button input{left:3px; vertical-align:top; cursor:pointer; padding:1px 12px 1px 10px; font-size:12px; font-weight:normal; height:29px; background-position:right top;}
.button *:hover,.button *:active,.button *:focus{color:#6e6e6e;}

/* Button - 기능버튼 */
.button.iconBtn,.button.iconBtn a,.button.iconBtn button,
.button.iconBtn input{font-size:12px; font-weight:normal; height:23px; background-image:url(../images/button/btn_basic03.png); color:#666666;}
.button.iconBtn *:hover,.button.iconBtn *:active,.button.iconBtn *:focus{color:#4e4e4e;}

/* Button - 팝업버튼 */
.button.popupBtn,.button.popupBtn a,.button.popupBtn button,
.button.popupBtn input{font-size:12px; font-weight:bold; height:24px; background-image:url(../images/button/btn_basic.gif); color:#4e4e4e;}
.button.popupBtn *:hover,.button.popupBtn *:active,
.button.popupBtn *:focus{color: #4e4e4e;}

/* Button - Grid 버튼 */
.button.gridBtn,.button.gridBtn a,.button.gridBtn button,
.button.gridBtn input{font-size:12px; line-height:17px; height:23px; background-image:url(../images/button/btn_grid_basic03.png); color:#fff; text-indent:0px;}
.button.gridBtn *:hover,.button.gridBtn *:active,
.button.gridBtn *:focus{color: #e32944;}

/* Button - Form 버튼 */
.button.formBtn,.button.formBtn a,.button.formBtn button,
.button.formBtn input{font-size:11px; height:23px; background-image:url(../images/button/btn_grid_basic02.png); color:#383838; text-indent:0px;}
.button.formBtn *:hover,.button.gridBtn *:active,
.button.formBtn *:focus{color: #5a5a5a;}

/* small 버튼(검색영역 기간, 달력, 돋보기 아이콘, 캘린더 기간, 로그인 영역) */
.button.smallBtn,.button.smallBtn a,.button.smallBtn button,
.button.smallBtn input{vertical-align:top; font-size:11px; height:21px; background-image:url(../images/button/btn_small.gif); color:#444444;}
.button.smallBtn *:hover,.button.smallBtn *:active,
.button.smallBtn *:focus{color: #444444;}

/* data 입력양식 버튼(우편번호 검색, 중복검색) */
.button.dataBtn,.button.dataBtn a,.button.dataBtn button,
.button.dataBtn input{font-size:11px; height:21px; background-image:url(../images/button/btn_small.gif); color:#444444;}
.button.dataBtn *:hover,.button.dataBtn *:active,.button.dataBtn *:focus{color:#775305;}

/* 검정 버튼(month, week, day, search */
.button.blackBtn,.button.blackBtn a,.button.blackBtn button,.button.blackBtn input{font-size:11px; font-weight:bold; height:20px; background-image:url(../images/button/btn_black_small.gif); color:#ffffff;}
.button.blackBtn *:hover,.button.blackBtn *:active,.button.blackBtn *:focus{color:#ffffff;}

/* 검정 버튼 Big(검색 버튼 등) */
.button.blackBtnBig{background-image: url(../images/button/btn_black_b.gif);}
.button.blackBtnBig a,.button.blackBtnBig button,.button.blackBtnBig input{font-size:12px; height:47px; padding-right:16px; padding-left:15px; background-image:url(../images/button/btn_black_b.gif); color:#ffffff;}
.button.blackBtnBig *:hover,.button.blackBtnBig *:active,.button.blackBtnBig *:focus{color:#ffffff;}
.button.blackBtn a .ui-button-text,
.button.blackBtn button .ui-button-text,
.button.blackBtn input .ui-button-text{padding: .2em .3em .1em 1.3em;}

/* 기능버튼 - 추가, 제거 */
.btnFunc.btnAdd a,.btnFunc.btnAdd button,.btnFunc.btnAdd input{font-size:12px; width:29px; height:27px; background-image:url(../images/button/btn_add.png); border:0;}
.btnFunc.btnAdd .button-text{display:none;}
.btnFunc.btnRemove a,.btnFunc.btnRemove button,.btnFunc.btnRemove input{font-size:12px; width:29px; height:27px; background-image:url(../images/button/btn_remove.png); border:0;}
.btnFunc.btnRemove .button-text{display:none;}

/* 이미지버튼 */
.btnImage{display:-moz-inline-stack; display:inline-block; height:27px; vertical-align:middle; }
.btnImage .button-text{display:none;}
.btnAppend02{font-size:12px; width:29px; height:27px; background-image:url(../images/button/btn_add.png); border:0;}
.btnRemove02{font-size:12px; width:29px; height:27px; background-image:url(../images/button/btn_remove.png); border:0;}
.btnArrowUp{font-size:12px; width:21px; height:23px; background-image:url(../images/button/btn_arrow_up.png); border:0;}
.btnArrowDown{font-size:12px; width:21px; height:23px; background-image:url(../images/button/btn_arrow_down.png); border:0;}
.btnArrowRight{font-size:12px; width:22px; height:21px; background-image:url(../images/button/btn_arrow_right.png); border:0;}
.btnArrowLeft{font-size:12px; width:23px; height:21px; background-image:url(../images/button/btn_arrow_left.png); border:0;}

/* Button Text Strong */
.button.strong * {font-weight:bold;}

/* 버튼 간격 조절 */
.button + .button{margin-left:3px;}
input + .button{margin-left:3px;}
.button + input{margin-left:3px;}
.btn_common .button + .button{margin-left:6px; *margin-left:3px;}

